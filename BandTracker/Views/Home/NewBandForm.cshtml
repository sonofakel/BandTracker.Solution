@{
  Layout ="_Layout";
}

<div class="jumbotron">
  <h1>Create a new band</h1>
</div>

<form action="/venue/@model.GetId()/band-added" method="post">
  <label for="band-name">Add a band</label>
  <input type="text" name="band-name">
  <button type="submit" class="btn btn-info">Add</button>
</form>
